%table
  - form_for[:admin, @order] do |f|
    %tr
      %td
        = f.label :id
        = f.text_field :id
      %td
        = f.label :created_at
        = f.text_field :created_at

%table
  - @order.products.each do |product|
    %tr
      %td
        = f.label :name
        = f.text_field product.name
      %td
        = f.label :quantity
        = 
      %td
        = f.label :price
        = f.text_field product.price 

